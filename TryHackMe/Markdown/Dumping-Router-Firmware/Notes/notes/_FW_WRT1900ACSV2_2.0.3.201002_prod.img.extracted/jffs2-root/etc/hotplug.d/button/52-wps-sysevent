if [ "$ACTION" = "released" -a "$BUTTON" = "wps" ]; then
	echo "" > /dev/console
	echo "WPS PUSH BUTTON -- GENERATING WPS_HW BUTTON EVENT " > /dev/console
        sysevent set wps_hw_button pressed
fi
