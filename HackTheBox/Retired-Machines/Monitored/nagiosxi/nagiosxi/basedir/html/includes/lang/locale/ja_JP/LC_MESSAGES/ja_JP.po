msgid "Host - first notification delay"
msgstr "ホスト - 最初の通知の遅延"

#: nagiosxi/basedir/html/includes/components/ccm/includes/language.inc.php:55
msgid ""
"This directive is used to define the number of \"time units\" to wait before"
